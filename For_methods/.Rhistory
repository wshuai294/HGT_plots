dat<-read.table("amount_comparison.csv",sep=',', header=TRUE)
pdf(file="swift.pdf", width=5, height=5, onefile=FALSE)
theme_set(theme_bw())
# new_dat
p1<- ggplot(dat, aes(x=fraction, y=CPU.time)) +
geom_line(aes(linetype=Complexity, color=Methods))+
geom_point(aes(shape=Complexity, color=Methods ))
p2<- ggplot(dat, aes(x=fraction, y=Peak.RAM)) +
geom_line(aes(linetype=Methods, color=Complexity))+
geom_point(aes(shape=Methods, color=Complexity ))
prow <- plot_grid(
p1 + theme(legend.position="none"),
p2 + theme(legend.position="none"),
align = 'vh',
hjust = -1,
nrow = 1
)
legend <- get_legend(
p1 +
guides(color = guide_legend(nrow = 1)) +
theme(legend.position = "top")
)
plot_grid(prow, legend, ncol = 1, rel_heights = c(1, .1))
dev.off()
